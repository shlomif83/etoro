pipeline {


//for run on master
     // Define agent details here
    agent {label "master"}
    stages{
        stage('pull code from SCM'){
            steps{
                bat'echo this job pull the code'
            }
        }

        stage('call to run script job'){
            steps{
                build job: 'run-script', parameters: [[$class: 'StringParameterValue', name: 'FIRAT_JOB_NAME', value: "${JOB_NAME}"],
                 [$class: 'StringParameterValue', name: 'FIRAT_BUILD_NUMBER', value: "${BUILD_NUMBER}"]]
            }    
        }
    }
}