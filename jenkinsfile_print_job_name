pipeline {


//for run on master
     // Define agent details here
    agent {label "master"}
    options {skipDefaultCheckout true}
    stages{
        stage('print job name'){
            steps{
                script{
                    bat 'set'
                    bat "python ..\\${FIRST_JOB_NAME}\\print_job_name.py"
                }
            }
        }
        stage('run print the build number of the first job'){
            steps{
                script{
                    build job: 'print-first-job-number', parameters: [name: 'FIRST_BUILD_NUMBER', value: "${FIRST_BUILD_NUMBER}"]]
                }
            }
        }
    }
}